{
  "name": "api",
  "scripts": {
    "dev": "TZ=UTC bun run --watch bin/run.ts server",
    "dev:test": "TZ=UTC bun run bin/run.ts setup && TZ=UTC bun run dev",
    "build": "mkdir -p build && cd build && bun build --compile ../bin/run.ts --outfile run",
    "codegen": "graphql-codegen",
    "typecheck": "tsc --noEmit",
    "setup": "TZ=UTC bun bin/run.ts setup",
    "migrate": "TZ=UTC bun bin/run.ts migrate",
    "migrate:create": "TZ=UTC bun bin/run.ts migrate:create",
    "migrate:rollback": "TZ=UTC bun bin/run.ts migrate:rollback",
    "migrate:up": "TZ=UTC bun bin/run.ts migrate:up",
    "migrate:down": "TZ=UTC bun bin/run.ts migrate:down",
    "seed": "TZ=UTC bun bin/run.ts seed",
    "job": "TZ=UTC bun bin/run.ts job"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.13",
    "@graphql-tools/utils": "^9.1.4",
    "@simplewebauthn/server": "^7.0.1",
    "dataloader": "^2.1.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.18.0",
    "graphql-yoga": "~3.1.0",
    "jose": "^4.11.2",
    "lodash": "^4.17.21",
    "postgres": "^3.3.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.4",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@simplewebauthn/typescript-types": "^7.0.0",
    "@types/cookie": "^0.5.1",
    "@types/lodash": "^4.14.182",
    "@types/minio": "^7.0.15",
    "bun-types": "canary",
    "typescript": "^4.9.4"
  }
}
